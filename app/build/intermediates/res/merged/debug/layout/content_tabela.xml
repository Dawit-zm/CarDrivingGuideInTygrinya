<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_tabela"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.gmail.development.app.des.cardrivingguideintygrinya.Tabela"
    tools:showIn="@layout/app_bar_tabela">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:id="@+id/abcd"
        android:layout_marginBottom="10sp">
        <ListView
            android:id="@+id/list"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:scrollIndicators="right"
            android:layout_below="@+id/txt2"
            android:layout_marginTop="2sp"
            android:paddingBottom="10sp"
            android:layout_alignParentBottom="true">
        </ListView>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Small Text"
            android:id="@+id/txt2"
            android:layout_gravity="center_horizontal"
            android:textColor="#000"
            android:layout_marginLeft="2sp"
            android:background="@drawable/textview_border"
            android:layout_alignParentEnd="true"
            android:padding="5sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/abc"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:visibility="gone"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        android:verticalScrollbarPosition="left"
        android:layout_marginBottom="20sp">

        <TextView
            android:layout_width="fill_parent"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Small Text"
            android:id="@+id/txt"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/textview_border"
            android:textColor="#000"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_height="wrap_content"
            android:scrollHorizontally="true"
            android:typeface="serif"
            android:textStyle="normal|italic"
            android:textSize="16sp"
            android:padding="5dp"
            android:layout_margin="2dp" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/my_custom_background"
            android:scrollIndicators="right"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/txt"
            android:layout_marginLeft="3sp"
            android:layout_marginRight="3sp"
            android:layout_marginTop="2sp"
            android:id="@+id/scrollView3"
            android:layout_above="@+id/button5"
            android:layout_marginBottom="2sp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"

                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Medium Text"
                            android:id="@+id/textView"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000"
                            android:scrollbars="vertical"
                            android:padding="10dp"

                            android:background="@drawable/textview_border"
                            android:typeface="serif"
                            android:paddingLeft="3sp"
                            android:textStyle="normal|italic"
                            android:textSize="20sp" />
                    </LinearLayout>

                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView3"
                    android:visibility="visible"
                    android:layout_gravity="center_horizontal" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1"
                    android:padding="5dp">

                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageButton"
                        android:background="@drawable/off"
                        android:onClick="btn1"
                        android:clickable="true"

                        android:contentDescription=""
                        tools:ignore="ContentDescription"
                        android:paddingLeft="2sp"
                        android:paddingTop="2sp"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Medium Text"
                        android:id="@+id/button"

                        android:clickable="true"

                        android:onClick="btn1"
                        android:gravity="center_vertical"
                        android:textColor="#000"
                        android:scrollHorizontally="true"

                        android:minLines="1"
                        android:textSize="16sp"
                        android:layout_weight="0.92"
                        android:typeface="serif"
                        android:paddingLeft="2sp"
                        android:textStyle="normal|italic"
                        android:paddingTop="2sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageButton2"
                        android:background="@drawable/off"
                        android:clickable="true"
                        android:onClick="btn2"
                        tools:ignore="ContentDescription"
                        android:paddingLeft="2sp"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Medium Text"
                        android:id="@+id/button2"
                        android:gravity="center_vertical"
                        android:onClick="btn2"
                        android:clickable="true"
                        android:textColor="#000"
                        android:scrollHorizontally="true"
                        android:minLines="1"
                        android:textSize="16sp"
                        android:layout_weight="0.92"
                        android:typeface="serif"
                        android:paddingLeft="2sp"
                        android:textStyle="normal|italic" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageButton3"
                        android:background="@drawable/off"
                        android:onClick="btn3"
                        android:clickable="true"
                        tools:ignore="ContentDescription"
                        android:paddingLeft="2sp"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Medium Text"
                        android:id="@+id/button3"
                        android:gravity="center_vertical"
                        android:onClick="btn3"
                        android:clickable="true"
                        android:textColor="#000"
                        android:scrollHorizontally="true"
                        android:minLines="1"
                        android:textSize="16sp"
                        android:layout_weight="0.92"
                        android:typeface="serif"
                        android:paddingLeft="2sp"
                        android:textStyle="normal|italic" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageButton4"
                        android:background="@drawable/off"
                        android:clickable="true"
                        android:onClick="btn4"
                        tools:ignore="ContentDescription"
                        android:paddingLeft="2sp"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Medium Text"
                        android:id="@+id/button4"
                        android:clickable="true"
                        android:gravity="center_vertical"
                        android:onClick="btn4"
                        android:textColor="#000"
                        android:minLines="1"
                        android:scrollHorizontally="true"
                        android:textSize="16sp"
                        android:layout_weight="0.92"
                        android:typeface="serif"
                        android:paddingLeft="2sp"
                        android:textStyle="normal|italic" />

                </LinearLayout>

            </LinearLayout>
        </ScrollView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Medium Text"
            android:onClick="btn3"
            android:clickable="true"
            android:textColor="#000"
            android:scrollHorizontally="true"
            android:minLines="1"
            android:textSize="14sp"
            android:layout_weight="0.92"
            android:typeface="serif"
            android:textStyle="normal|italic"
            android:gravity="left"
            android:id="@+id/textView2"
            android:layout_alignBaseline="@+id/txt"
            android:layout_alignBottom="@+id/txt"
            android:layout_alignEnd="@+id/txt"
            android:paddingRight="5dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ab"
        android:visibility="visible"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tabela101"
            android:id="@+id/button6"
            android:onClick="Acc101"
            android:typeface="serif"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textSize="18sp"
            android:textStyle="normal|italic" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tabela105"
            android:id="@+id/button10"
            android:onClick="Acc105"
            android:typeface="serif"
            android:textSize="18sp"
            android:textStyle="normal|italic"
            android:layout_below="@+id/button9"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/tabela102"
            android:id="@+id/button7"
            android:onClick="Acc102"
            android:typeface="serif"
            android:textStyle="normal|italic"
            android:textSize="18sp"
            android:layout_below="@+id/button6"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/tabela103"
            android:id="@+id/button8"
            android:onClick="Acc103"
            android:typeface="serif"
            android:textStyle="normal|italic"
            android:textSize="18sp"
            android:layout_below="@+id/button7"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/tabela104"
            android:id="@+id/button9"
            android:onClick="Acc104"
            android:typeface="serif"
            android:textStyle="normal|italic"
            android:textSize="18sp"
            android:layout_below="@+id/button8"
            android:layout_alignParentStart="true" />
    </RelativeLayout>
</RelativeLayout>
